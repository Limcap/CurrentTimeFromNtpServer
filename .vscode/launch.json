	"version": "0.2.0",
	"configurations": [
		{
			"name": "TypeScript",
			"type": "pwa-node",
			"request": "launch",
			"skipFiles": [
				"<node_internals>/**"
			],
			"program": "${workspaceFolder}\\typescript\\src\\index.ts",
			"outFiles": [
				"${workspaceFolder}/**/*.js"
			]
		},
		{
			"name": "Dart",
			"request": "launch",
			"type": "dart",
			"program": "dart\\main.dart",
		}
	]
}